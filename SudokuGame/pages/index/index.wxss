.container{
  padding: 100rpx 0;
}
.rows {
  display: flex;
  flex-direction: row;
  justify-content: space-around;
}

.cols {
  width: 75rpx;
  height: 75rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  /* border: 1px solid #000; */
}

.row_g_top:first-child {
  border-top: 2px solid #000;
}

.row_g_bottom {
  border-bottom: 2px solid #000;
}

.row_g_middle, .row_g_bottom {
  border-top: 1px solid #000;
}

.col_g_left {
  border-left: 2px solid #000;
}

.col_g_right:last-child {
  border-right: 2px solid #000;
}

.col_g_center, .col_g_right {
  border-left: 1px solid #000;
}

.empty{
  color:#fff;
}
.fixed{
  background-color: #eee;
}
.dashboard{
  margin-top: 100rpx;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}
.dashboard button{
  background-color: #2EBF76;
  color: #fff;
  font-size: 35rpx;
}
.dashboard button+button{
  margin-left: 60rpx;
}